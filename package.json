{
  "name": "dirbuster",
  "version": "0.0.5",
  "description": "dirbuster implementation in Node.js with some extrasauce",
  "bin": {
    "dirbuster": "bin/cli.js"
  },
  "main": "src/index.js",
  "scripts": {
